#/bin/bash

echo -e "\ 033 [1; 33m- - - - -> \ 033 [01; 34m Iniciando configuracion ZiProxy, Puerto 8081 CONNECT"
echo -e "\ 033 [1; 33m #################"
sleep 3

clear
apt-get install ziproxy -y
clear

cd /etc/ziproxy/
mv ziproxy.conf ziproxy.confx
wget https://raw.githubusercontent.com/powermx/zi/master/ziproxy.conf
/etc/init.d/ziproxy restart
clear

echo -e "\ 033 [1; 31mModulo VPSPack"
